{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "hello-cafeteria-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/helloCafeteria/HelloCafeteriaAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/helloCafeteria/HelloCafeteriaAdaptiveCardExtension.manifest.json"
        }
      ]
    },
    "hello-cafeteria-bonus-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/helloCafeteriaBonus/HelloCafeteriaBonusAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/helloCafeteriaBonus/HelloCafeteriaBonusAdaptiveCardExtension.manifest.json"
        }
      ]
    },
    "cafeteria-4-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/cafeteria4/Cafeteria4AdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/cafeteria4/Cafeteria4AdaptiveCardExtension.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "HelloCafeteriaAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/helloCafeteria/loc/{locale}.js",
    "HelloCafeteriaBonusAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/helloCafeteriaBonus/loc/{locale}.js",
    "Cafeteria4AdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/cafeteria4/loc/{locale}.js"
  }
}
